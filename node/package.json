    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tslint": "6.1.3",
    "typescript": "5.7.2",
    "webpack": "5.99.8",
    "webpack-cli": "4.10.0",
    "webpack-filter-warnings-plugin": "1.2.1",
    "webpack-node-externals": "3.0.0"
  },
