{
  "name": "@malkab/rxredis",
  "version": "2.1.0",
  "description": "A RxJS Redis interface.",
  "scope": "@malkab",
  "author": {
    "name": "Juan Pedro Perez Alcantara",
    "email": "jp.perez.alcantara@gmail.com",
    "url": "http://me.com"
  },
  "main": "dist/main.js",
  "types": "dist/lib/index.d.ts",
  "keywords": [
    "Redis",
    "RxJS"
  ],
  "license": "UNLICENSED",
  "homepage": "https://github.com/owner/project#readme",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/malkab/code-samples.git"
  },
  "private": false,
  "scripts": {
    "clean": "rm -Rf build ; rm -Rf dist",
    "quick-test": "npm-run-all --parallel service:watch:quick-test:server service:build",
    "start": "npm-run-all --parallel service:watch:main:server service:build",
    "mocha": "npm-run-all --parallel service:watch:mocha:server service:build",
    "build": "npm run clean ; npm run service:build-main",
    "build-with-docs": "npm run clean ; npm run service:build-main ; npm run builddocs ; cp -R typedoc/build/html dist/docs",
    "build-docs": "cd typedoc ; ./html.sh ; cd ..",
    "service:build-main": "webpack --config webpack-production.config.js --progress --colors",
    "service:build": "webpack --progress --colors",
    "service:watch:quick-test:server": "nodemon --inspect=0.0.0.0:9329 \"./build/quicktest.js\" --watch \"./build/quicktest.js\"",
    "service:watch:mocha:server": "nodemon --ext js --watch ./build/mocha.js --exec 'mocha --timeout 20000 ./build/mocha.js || true'",
    "service:watch:main:server": "nodemon --ext js --watch ./build/main.js --exec 'mocha --timeout 20000 ./build/main.js || true'"
  },
  "engines": {
    "node": "^12.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.0.0",
    "@types/mocha": "^7.0.0",
    "@types/node": "^14.0.0",
    "@types/redis": "^2.8.13",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "terser": "^4.0.0",
    "terser-webpack-plugin": "^3.0.0",
    "ts-loader": "^7.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^6.0.0",
    "typedoc": "^0.17.0",
    "typescript": "^3.7.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@malkab/ts-utils": "^1.0.0",
    "redis": "^3.0.2",
    "rxjs": "^6.5.2"
  }
}
